[user]
	name = Peter VandeHaar
	email = <<removed>>
[credential]
	helper = osxkeychain
[push]
	default = simple
[core]
	excludesfile = /Users/peter/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[alias]
	cdiff = diff --color-words=.
	wdiff = diff --word-diff
[diff]
	algorithm = patience
[color]
	ui = true
